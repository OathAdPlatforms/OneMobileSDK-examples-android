<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_sdk_initialization_errors"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/sdk_initialization_errors" />

    <Button
        android:id="@+id/btn_player_initialization_errors"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/player_initialization_errors" />

    <Button
        android:id="@+id/btn_deleted_video"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/deleted_video" />

    <Button
        android:id="@+id/btn_invalid_video"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/invalid_video" />

    <Button
        android:id="@+id/btn_restricted_video"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/restricted_video" />

</LinearLayout>
